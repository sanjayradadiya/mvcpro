<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    @Styles.Render("~/Content/css")
    <link rel="shortcut icon" type="image/png" href="~/Content/Images/favicon.ico" />
    <link href="~/Content/Fonts/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/Content/toastr.min.css" rel="stylesheet" />
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    <script type="text/javascript">
        $(window).load(function () { $(".se-pre-con").fadeOut("slow"); });
        $(function () {
            $(".dropdown").hover(
                function () { $('.dropdown-menu', this).stop(true, true).fadeIn("fast"); $(this).toggleClass('open'); $('b', this).toggleClass("caret caret-up"); },
                function () { $('.dropdown-menu', this).stop(true, true).fadeOut("fast"); $(this).toggleClass('open'); $('b', this).toggleClass("caret caret-up"); });
        });
    </script>
</head>
<body>
    @*<div class="se-pre-con"></div>*@
    <nav class="navbar navbar-default topheader" role="navigation">
        <div class="header navbar-fixed-top">
            <div class="navbar-header">
                <a class="navbar-brand logo" href="#"></a>
                <button type="button" class="navbar-toggle"
                    data-toggle="collapse" data-target="#inv-navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>

            <div class="collapse navbar-collapse" id="inv-navbar-collapse">
                <ul class="nav navbar-nav navbar-left"></ul>
                <ul class="nav navbar-nav navbar-right">
                    <li class="dropdown"><a href="@Url.Action("Index", "InventorySearch")" title="New Search"><i class="fa fa-search fa-2x facolor faa-ring" aria-hidden="true"></i><span class="profilenames facolor invsrch">New Search</span></a></li>
                    @*<li class="dropdown menuhide"><a href="#" title="Notifaction"><i class="fa fa-bell-o fa-2x facolor faa-ring" aria-hidden="true"></i><span class="badge">2</span></a></li>
                    <li class="menuhide"><a href="#" title="Event"><i class="fa fa-calendar fa-2x facolor" aria-hidden="true"></i><span class="badge">0</span></a></li>
                    <li class="dropdown menuhide"><a href="#" title="E-Mail"><i class="fa fa-envelope-o fa-2x facolor" aria-hidden="true"></i><span class="badge">50</span></a></li>*@
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle profilenames" data-toggle="dropdown"><span class="fa-titles"><i class="fa fa-user facolor" aria-hidden="true"></i></span><span class="facolor" style="padding-left: 4px;">@Session["FirstName"] </span><i class="fa fa-caret-down facolor" aria-hidden="true"></i></a>
                        <ul class="dropdown-menu">
                            <li><a href="@Url.Action("UploadCart", "InventoryCart")"><i class="fa fa-cloud-upload fa-fw facolor " aria-hidden="true"></i>&nbsp;<span class="facolor"> Import Cart / Orders </span></a></li>
                            <li><a href="@Url.Action("BookAppointment", "Account")"><i class="fa fa-calendar fa-fw facolor " aria-hidden="true"></i>&nbsp;<span class="facolor"> Book Appointment</span></a></li>
                            <li><a href="@Url.Action("MyCart", "InventoryCart")" ><i class="fa fa-shopping-cart fa-fw facolor " aria-hidden="true"></i>&nbsp;<span class="facolor"> My Cart</span></a></li>
                            <li><a href="@Url.Action("MyBuyRequest", "InventoryCart")" ><i class="fa fa-eye fa-fw facolor " aria-hidden="true"></i>&nbsp;<span class="facolor"> My Orders</span></a></li>
                            @*<li class="divider"></li>
                            <li><a href="javscript:void(0);"><i class="fa fa-clock-o fa-fw fa-spin facolor " aria-hidden="true"></i>&nbsp;<span class="facolor"> My DDC</span></a></li>
                            <li class="divider"></li>*@
                            <li><a href="@Url.Action("ComingSoon", "Error")"><i class="fa fa-search-plus fa-fw facolor " aria-hidden="true"></i>&nbsp;<span class="facolor"> My Saved Search </span></a></li>
                            @*<li><a href="javscript:void(0);"><i class="fa fa-inbox fa-fw facolor " aria-hidden="true"></i>&nbsp;<span class="facolor"> MY MGS Inbox</span></a></li>*@
                            <li><a href="@Url.Action("EditProfile", "Account")"><i class="fa fa-cog fa-spin fa-fw facolor " aria-hidden="true"></i>&nbsp;<span class="facolor">  Account Settings</span></a></li>
                            <li class="divider"></li>
                            <li>
                                <button id="btnReset" class="btn btn-warning " onclick="location.href='@Url.Action("LogOff", "Account")'"  style="margin-left: 10%; border-color: #5362be; background-color: #5c6bc0; color: white;"><i class="fa fa-power-off fa-fw" aria-hidden="true"></i>&nbsp;<span class="facolor"> SIGN OUT</span></button></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="se-pre-con" style="background: url('../../Content/Images/mg.gif') center center no-repeat rgb(255,255,255);"></div>
    @*------------------------------Section Start*@
    <div style="min-height: 100%; bottom: 0px;">
        <section>
            <div id="section">
                @RenderBody()
            </div>
        </section>
    </div>
    <div class="clearfix"></div>
    @*------------------------------Footer Start*@
    <div class="footer">
        <div id="footer">
            <div class="copy">
                © Silicon Infovision Pvt Ltd. All Rights Reserved @DateTime.Today.Year
            </div>
            <div class="poweredby">
                Powered by <a href="http://siliconinfovision.com">Silicon Infovision Pvt Ltd.</a>
            </div>
        </div>
    </div>
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    <script src="~/Scripts/toastr.min.js"></script>
    <script type="text/javascript" src="~/Scripts/Custom/jscommon.js"></script>
    <script type="text/javascript">
        $(function () {
            function startWorker() { if (typeof (Worker) !== "undefined") { if (typeof (w) == "undefined") { w = new Worker("../../Scripts/Custom/aliveSessions.js"); } /*w.onmessage = function (event) { document.getElementById("result").innerHTML = event.data; }; } /*else { document.getElementById("result").innerHTML = "Sorry, your browser does not support Web Workers...";*/ } }
            startWorker();
        });
    </script>
    @*<link href="~/Js-alert/themes/default/theme.min.css" rel="stylesheet" />
    <script src="~/Js-alert/js/alert.min.js"></script>
    <link href="~/Js-alert/css/alert.min.css" rel="stylesheet" />*@
</body>
</html>
